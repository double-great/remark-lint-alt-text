{
  "name": "@double-great/remark-lint-alt-text",
  "version": "1.0.0",
  "description": "A remark-lint plugin that finds unhelpful or missing image alt text",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run lint",
    "test": "jest",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install",
    "prepublishOnly": "tsc"
  },
  "author": "@double-great",
  "license": "MIT",
  "dependencies": {
    "@double-great/alt-text": "^3.1.0",
    "unified-lint-rule": "^2.1.1",
    "unist-util-visit-parents": "^5.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.30.5",
    "dedent": "^0.7.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "remark": "^14.0.2",
    "typescript": "^4.7.4"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "accessibility",
    "remark",
    "remark-plugin",
    "remark-lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/double-great/remark-lint-alt-text.git"
  },
  "bugs": {
    "url": "https://github.com/double-great/remark-lint-alt-text/issues"
  },
  "homepage": "https://github.com/double-great/remark-lint-alt-text#readme",
  "jest": {
    "transformIgnorePatterns": [
      "!node_modules/"
    ],
    "moduleNameMapper": {
      "^../index.js$": "<rootDir>/index.ts"
    }
  }
}
